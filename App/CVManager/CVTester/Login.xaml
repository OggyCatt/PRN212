<Window x:Class="CVTester.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CVTester"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="775" Background="#555555" WindowStartupLocation="CenterScreen">
    <Grid>
        <Rectangle Stroke="Black" Margin="78,68,79,68" Fill="#7fffd4" RadiusX="20" RadiusY="20"/>

        <Button x:Name="LoginButton" Content="Login" HorizontalAlignment="Left" Margin="188,300,0,0" VerticalAlignment="Top" Height="38" Width="118" Foreground="White" Background="#080808" 
                FontSize="14" FontWeight="Bold" Click="Button_Click"/>
        <Button x:Name="CanncelButton" Content="Canncel" HorizontalAlignment="Left" Margin="476,300,0,0" VerticalAlignment="Top" Height="38" Width="116" Foreground="White" Background="#080808" 
            FontSize="14" FontWeight="Bold" Click="CanncelButton_Click" />

    </Grid>
</Window>
